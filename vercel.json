{
  "version": 2,
  "builds": [
    { "src": "server.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/", "dest": "/public/index.html" },
    { "src": "/about", "dest": "/public/about.html" },
    { "src": "/help", "dest": "/public/help.html" },
    { "src": "/(.*)", "dest": "/server.js" }
  ]
}
